{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 4,
  "useTabs": false,
  "trailingComma": "none",
  "printWidth": 100,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
